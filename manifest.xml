<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp
  xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:type="MailApp">

  <Id>d6ff5248-95b4-4400-8f5d-4ce60c4ed0be</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>KOMBIT</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>

  <DisplayName DefaultValue="Meeting Template"/>
  <Description DefaultValue="Automatic meeting template insertion"/>
  <IconUrl DefaultValue="https://icy-hill-0716f1a03.1.azurestaticapps.net/assets/icon-64.png"/>
  <HighResolutionIconUrl DefaultValue="https://icy-hill-0716f1a03.1.azurestaticapps.net/assets/icon-128.png"/>
  <SupportUrl DefaultValue="https://icy-hill-0716f1a03.1.azurestaticapps.net"/>

  <AppDomains>
    <AppDomain>https://icy-hill-0716f1a03.1.azurestaticapps.net</AppDomain>
  </AppDomains>

  <Hosts>
    <Host Name="Mailbox"/>
  </Hosts>

  <Requirements>
    <Sets>
      <Set Name="Mailbox" MinVersion="1.1"/>
    </Sets>
  </Requirements>

  <FormSettings>
    <Form xsi:type="ItemEdit">
      <DesktopSettings>
        <SourceLocation DefaultValue="https://icy-hill-0716f1a03.1.azurestaticapps.net/taskpane.html"/>
        <RequestedHeight>250</RequestedHeight>
      </DesktopSettings>
    </Form>
  </FormSettings>

  <Permissions>ReadWriteItem</Permissions>

  <Rule xsi:type="RuleCollection" Mode="Or">
    <Rule xsi:type="ItemIs" ItemType="Appointment" FormType="Edit"/>
    <Rule xsi:type="ItemIs" ItemType="Appointment" FormType="ReadOrEdit"/>
  </Rule>

  <DisableEntityHighlighting>false</DisableEntityHighlighting>

</OfficeApp>
